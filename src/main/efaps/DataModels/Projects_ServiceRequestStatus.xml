<datamodel-statusgroup  xmlns="http://www.efaps.org/xsd"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>a028ae03-b453-4b0b-b051-f7f2d5e81cf9</uuid>
    <file-application>eFapsApp-Projects</file-application>
    <file-revision>$Rev$</file-revision>
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Projects_ServiceRequestStatus</name>
        <parent>Projects_DocumentStatus</parent>
        <status key="Open">
            <description>Service Request was printed, but no project created yet, because client must confirm.</description>
        </status>
        <status key="Accepted">
            <description>Service Request was accepted by the client. Project was created.</description>
        </status>
        <status key="Closed">
            <description>Service Request was closed successfully.</description>
        </status>
        <status key="Canceled">
            <description>Service Request was canceled.</description>
        </status>
    </definition>
</datamodel-statusgroup>

