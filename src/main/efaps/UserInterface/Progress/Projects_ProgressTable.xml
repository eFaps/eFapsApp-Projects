<?xml version="1.0" encoding="UTF-8"?>




<ui-table xmlns="http://www.efaps.org/xsd"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>bfce1bab-44a8-4f85-a970-daa263afa6b4</uuid>
    <file-application>eFapsApp-Projects</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Projects_ProgressTable</name>
        

        <field name="type">
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Attribute">Type</property>
            <property name="Label">Projects_ProgressAbstract/Type.Label</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="description">
            <property name="Attribute">Description</property>
            <property name="Label">Projects_ProgressAbstract/Description.Label</property>
        </field>
        <field name="date">
            <property name="Attribute">Date</property>
            <property name="Label">Projects_ProgressAbstract/Date.Label</property>
        </field>
        <field name="progress">
            <property name="Attribute">Progress</property>
            <property name="Label">Projects_ProgressAbstract/Progress.Label</property>
        </field>
         <field name="uoM">
            <property name="Select">linkto[UoM].attribute[Name]</property>
            <property name="Label">Projects_ProgressAbstract/UoM.Label</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
            <property name="Width">15px</property>
        </field>
    </definition>
</ui-table>
