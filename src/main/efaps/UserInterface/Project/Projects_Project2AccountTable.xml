            <property name="Label">Projects_Project2AccountTable/Type.Label</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="account">
            <property name="SelectAlternateOID">linkto[ToLink].oid</property>
            <property name="Select">linkto[ToLink].attribute[Name]</property>
            <property name="Label">Projects_Project2AccountTable/Account.Label</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
            <property name="Width">15px</property>
        </field>
    </definition>
</ui-table>
