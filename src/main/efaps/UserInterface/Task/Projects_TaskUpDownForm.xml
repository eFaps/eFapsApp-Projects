<?xml version="1.0" encoding="UTF-8" standalone="no"?>


<ui-form xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>019f6b2b-54ff-407a-8806-887c6b0df780</uuid>
    <file-application>eFapsApp-Projects</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Projects_TaskUpDownForm</name>
        <field name="selectedOID">
            <property name="ModeCreate">HIDDEN</property>
            <property name="ModeView">NONE</property>
            <property name="ModeEdit">HIDDEN</property>
            <property name="UIProvider">org.efaps.admin.datamodel.ui.StringUI</property>
            <trigger event="UI_FIELD_VALUE" method="getStoreOIDsFieldValue"
                name="Projects_TaskUpDownForm.parentOID.UI_FIELD_VALUE"
                program="org.efaps.esjp.common.uiform.Field" />
        </field>
        <field name="count">
            <property name="Columns">5</property>
            <property name="ModeCreate">EDITABLE</property>
            <property name="ModeEdit">EDITABLE</property>
            <property name="Label">Projects_TaskUpDownForm.count.Label</property>
            <property name="Required">true</property>
            <property name="UIProvider">org.efaps.admin.datamodel.ui.StringUI</property>
        </field>
        <field name="upDown">
            <property name="Columns">5</property>
            <property name="ModeCreate">EDITABLE</property>
            <property name="ModeEdit">EDITABLE</property>
